{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "ctf0/media-manager": "^1.0",
        "cybercog/laravel-likeable": "^3.1",
        "hekmatinasser/verta": "^1.8",
        "joselfonseca/image-manager": "2.1.*",
        "larabook/gateway": "^3.3",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "v5.*",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "2.1.7",
        "nesbot/carbon": "^1.22",
        "saeedpooyanfar/zarinpal": "^3.0",
        "thetispro/laravel5-setting": "1.1.x-dev",
        "unisharp/laravel-ckeditor": "^4.6",
        "unisharp/laravel-filemanager": "~1.8",
      "ext-http": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "App\\Providers\\EventServiceProvider::postAutoloadDump"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [

            ]
        }
    }
}
